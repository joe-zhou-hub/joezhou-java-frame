<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dogSpace">

    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO `dog`
        VALUES (#{id}, #{name}, #{gender}, #{age}, #{info})
    </insert>

</mapper>